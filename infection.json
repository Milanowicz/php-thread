{
  "source": {
    "directories": [
      "src"
    ]
  },
  "logs": {
    "text": "testing/infection.log",
    "html": "testing/infection.html",
    "summary": "testing/infection-summary.log",
    "json": "testing/infection-log.json",
    "perMutator": "testing/infection-mutator.md",
    "github": true,
    "stryker": {
      "badge": "master"
    }
  },
  "mutators": {
    "@default": true,
    "global-ignoreSourceCodeByRegex": [
      ".*exec.*",
      ".*shell.*",
      ".*PHP_EOL.*"
    ]
  }
}
